<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="borderPane" styleClass="background" stylesheets="/appStyle.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
         <center>
            <ListView fx:id="listView" maxWidth="700.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets top="30.0" />
         </BorderPane.margin></ListView>
         </center>
   <bottom>
      <BorderPane maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
         <left>
                <Button fx:id="editScenarioButton" disable="true" mnemonicParsing="false" prefHeight="26.0" text="Edytuj scenariusz" BorderPane.alignment="CENTER">
                 <BorderPane.margin>
                     <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
                 </BorderPane.margin></Button>
         </left>
         <center>

                   <Button fx:id="runScenarioButton" disable="true" mnemonicParsing="false" prefHeight="26.0" text="Uruchom scenariusz" />



         </center>
         <right>
                   <Button fx:id="newScenarioButton" mnemonicParsing="false" prefHeight="26.0" text="Dodaj scenariusz" BorderPane.alignment="CENTER">
               <padding>
                  <Insets right="20.0" />
               </padding>
               <BorderPane.margin>
                  <Insets bottom="20.0" left="10.0" right="20.0" top="20.0" />
               </BorderPane.margin></Button>
         </right>
         <bottom>
            <Button fx:id="deleteButton" disable="true" mnemonicParsing="false" prefHeight="26.0" text="Usuń scenariusz" BorderPane.alignment="CENTER" />
         </bottom>
         <top>
            <Button fx:id="additionalFeaturesButton" mnemonicParsing="false" text="Dodatkowe Funkcjonalności" BorderPane.alignment="CENTER" />
         </top>
      </BorderPane>
   </bottom>
   <top>
      <Label fx:id="titleLabel" text="Centrum Symulacji" BorderPane.alignment="CENTER" />
   </top>
      </BorderPane>
